{
	"name": "kio-ng2-video",
	"version": "0.0.0",
	"description": "Video component",
	"main": "./release/video.module",
	"module": "./release/video.module",
	"typings": "./release/video.module.d.ts",
	"scripts": {
		"test": "ts-mocha ./src/**/*.spec.ts",
		"pretest": "npm run build",
		"test:watch": "./scripts/watch.sh test",
		"lint": "tslint ./src/**/*.ts ./test/**/*.ts",
		"version": "npm run build",
		"postversion": "git push && git push --tags",
		"predoc": "rm -rf ./documentation",
		"doc": "compodoc -p ./tsconfig-doc.json -d ./documentation/",
		"sass:build": "./scripts/compile-scss.sh ./build",
		"transpile": "npm run ngModule:build",
		"ngModule:build": "./scripts/ngModule.sh build",
		"ngModule:copy_build": "./scripts/ngModule.sh copy_build",
		"ngModule:copy": "./scripts/sync_to.sh ../../digit-wmr",
		"fetchdeps": "echo ./scripts/sync_kio_module.sh component-routing ctn i18n",
		"build": "npm run ngModule:copy_build && npm run sass:build && npm run ngModule:build && npm run ngModule:copy",
		"build:dev": "npm run fetchdeps && NODE_ENV=production npm run build",
		"build:release": "NODE_ENV=production npm run build",
		"build:release:watch": "./scripts/watch.sh build:release"
	},
